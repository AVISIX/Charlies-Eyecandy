{
    "author": "Sixmax",
    "name": "Charlies Eyecandy",
    "colors": {
      "editor.background": "#010123",
      "editor.foreground": "#6782fc",
      "editorLineNumber.foreground": "#01695C"
    },

    // https://macromates.com/manual/en/language_grammars

    "tokenColors": 
    [
/*
  _____            _                       
 / ____|          | |                      
| |     __ _ _ __ | |_ _   _ _ __ ___  ___ 
| |    / _` | '_ \| __| | | | '__/ _ \/ __|
| |___| (_| | |_) | |_| |_| | | |  __/\__ \
 \_____\__,_| .__/ \__|\__,_|_|  \___||___/
            | |                            
            |_|                 
*/

      {
        "name": "Function Name",
        "scope": 
        [
          "entity.name.function.lua"
        ],
        "settings": 
        {
          "foreground": "#B72525"
        }
      },

      {
        "name": "Paranthasis",
        "scope": 
        [
          "punctuation.separator.parameter.lua",
          "punctuation.definition.parameters.begin.lua",
          "punctuation.definition.parameters.end.lua",
          "punctuation.separator.arguments.lua"
        ],
        "settings": 
        {
          "foreground": "#FFFFFF"
        }
      },

      {
        "name": "Function Parameters",
        "scope": 
        [
          "variable.parameter.function.lua"
        ],
        "settings": 
        {
          "foreground": "#586C87"
        }
      },



/*
 _   _                 _                   
| \ | |               | |                  
|  \| |_   _ _ __ ___ | |__   ___ _ __ ___ 
| . ` | | | | '_ ` _ \| '_ \ / _ \ '__/ __|
| |\  | |_| | | | | | | |_) |  __/ |  \__ \
|_| \_|\__,_|_| |_| |_|_.__/ \___|_|  |___/
                                           
*/
      {
        "name": "Numbers",
        "scope": [
          "constant.numeric.integer.hexadecimal.lua",
          "constant.numeric.float.hexadecimal.lua",
          "constant.numeric.integer.lua",
          "constant.numeric.float.lua"
        ],
        "settings": 
        {
          "foreground": "#dbab43"
        }
      },



/*
  _____ _        _                 
 / ____| |      (_)                
| (___ | |_ _ __ _ _ __   __ _ ___ 
 \___ \| __| '__| | '_ \ / _` / __|
 ____) | |_| |  | | | | | (_| \__ \
|_____/ \__|_|  |_|_| |_|\__, |___/
                          __/ |    
                         |___/     
*/
      {
        "name": "Strings",
        "scope": [
          "string.quoted.single.lua",
          "string.quoted.other.multiline.lua",
          "string.quoted.double.lua"
        ],
        "settings": 
        {
          "foreground": "#5a6c75"
        }       
      },



/*
  _____                                     _       
 / ____|                                   | |      
| |     ___  _ __ ___  _ __ ___   ___ _ __ | |_ ___ 
| |    / _ \| '_ ` _ \| '_ ` _ \ / _ \ '_ \| __/ __|
| |___| (_) | | | | | | | | | | |  __/ | | | |_\__ \
 \_____\___/|_| |_| |_|_| |_| |_|\___|_| |_|\__|___/
                                                    
                                                    
*/
      {
        "name": "Comment",
        "scope": [
          "comment.line.double-dash.lua",
          "comment.block.lua"
        ],
        "settings": 
        {
          "foreground": "#636363"
        }       
      },
      

/*
  _____       _        
 / ____|     | |       
| |  __  ___ | |_ ___  
| | |_ |/ _ \| __/ _ \ 
| |__| | (_) | || (_) |
 \_____|\___/ \__\___/ 
                       
                       
*/

      {
        "name": "Goto Label",
        "scope": [
          "constant.other.placeholder.lua"
        ],
        "settings": 
        {
          "foreground": "#e04ff7",
          "fontstyle": "bold"
        }          
      },

/*
 _  __                                _     
| |/ /                               | |    
| ' / ___ _   ___      _____  _ __ __| |___ 
|  < / _ \ | | \ \ /\ / / _ \| '__/ _` / __|
| . \  __/ |_| |\ V  V / (_) | | | (_| \__ \
|_|\_\___|\__, | \_/\_/ \___/|_|  \__,_|___/
           __/ |                            
          |___/    
*/
      {
        "name": "Keywords",
        "scope": [
          "keyword.control.lua",
          "constant.language.lua",
          "keyword.control.goto.lua",
          "storage.modifier.local.lua",
          "keyword.control.lua"
        ],
        "settings": 
        {
          "foreground": "569CD6" // "#0be5de" 
        }          
      },
      {
        "name": "Lua-Self",
        "scope": [
          "variable.language.self.lua"
        ],
        "settings": 
        {
          "foreground": "#f2c3a4"
        }          
      },



      
/*
 ______                _   _                 
|  ____|              | | (_)                
| |__ _   _ _ __   ___| |_ _  ___  _ __  ___ 
|  __| | | | '_ \ / __| __| |/ _ \| '_ \/ __|
| |  | |_| | | | | (__| |_| | (_) | | | \__ \
|_|   \__,_|_| |_|\___|\__|_|\___/|_| |_|___/
                                             
                                             
*/
      {
        "name": "Functions",
        "scope": [
          "support.function.lua",
          "support.function.library.lua",
          "support.function.any-method.lua",
          "variable.other.lua"
        ],
        "settings": 
        {
          "foreground": "#898989"
        }          
      },      


/*
  ____                       _                 
 / __ \                     | |                
| |  | |_ __   ___ _ __ __ _| |_ ___  _ __ ___ 
| |  | | '_ \ / _ \ '__/ _` | __/ _ \| '__/ __|
| |__| | |_) |  __/ | | (_| | || (_) | |  \__ \
 \____/| .__/ \___|_|  \__,_|\__\___/|_|  |___/
       | |                                     
       |_|  
*/
      {
        "name": "Operators",
        "scope": [
          "keyword.operator.lua",
          "comment.line.shebang.lua",
          "string.add"
        ],
        "settings": 
        {
          "foreground": "#d1d843" //fc8e62
        }          
      },

      {
        "name": "Default",
        "scope": 
        [
          "punctuation.specials",
          "punctuation.parens"
        ],
        "settings": 
        {
          "foreground": "#FFFFFF"
        }
      },

      {
        "name": "Brackets",
        "scope": 
        [
          "punctuation.brackets"
        ],
        "settings":
        {
          "foreground": "#DF6F3F"
        }
      },

      // OTHER STUFF

      {
        "name": "Variables",
        "scope": 
        [
          "variable.default"
        ],
        "settings": 
        {
          "foreground": "#EEEEEE" 
        }
      }
    ]
  }